{
  "manifest_version": 2,
  "name": "webRequest",
  "version": "0.1.0",
  "description": "webRequest请求拦截器",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "background": {
    "page": "background.html",
    "persistent": true
  }
}